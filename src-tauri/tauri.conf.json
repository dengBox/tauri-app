{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm serve",
    "devPath": "http://localhost:2999",
    "distDir": "../dist"
  },
  "package": {
    "productName": "tauri-example",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http":{
        "scope":[
          "http://**",
          "https://**"
        ]
      },
      "fs": {
        "all": true,
        "scope": [
          "$RESOURCE/*"
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev1",
      "longDescription": "",
      "macOS": {
        "exceptionDomain": null,
        "entitlements": null,
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "resources/*"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "Tauri App",
        "url": "http://devhaina.shuhaisc.io",
        "width": 1440,
        "height": 900,
        "maxWidth": 767,
        "fullscreen": false,
        "resizable": true,
        "center": true
      }
    ]
  }
}